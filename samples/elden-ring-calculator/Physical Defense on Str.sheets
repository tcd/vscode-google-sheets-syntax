=IF(Calculator!$B$2>60,
        ADD(30,MULTIPLY(10,DIVIDE(MINUS(Calculator!$B$2,60),39))),
        IF(Calculator!$B$2>40,
        ADD(15,MULTIPLY(15,DIVIDE(MINUS(Calculator!$B$2,40),20))),
        IF(Calculator!$B$2>30,
        ADD(10,MULTIPLY(5,DIVIDE(MINUS(Calculator!$B$2,30),10))),
        MULTIPLY(10,DIVIDE(Calculator!$B$2,30)))))