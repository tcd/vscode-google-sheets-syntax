{
    "name": "vscode-google-sheets-syntax",
    "displayName": "vscode-google-sheets-syntax",
    "description": "Syntax highlighting for Google Sheets formulas.",
    "version": "3.0.0",
    "publisher": "dunstontc",
    "repository": {
        "type": "git",
        "url": "https://github.com/tcd/vscode-google-sheets-syntax.git"
    },
    "bugs": {
        "url": "https://github.com/tcd/vscode-google-sheets-syntax/issues"
    },
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "google-sheets",
                "aliases": [
                    "Google Sheets",
                    "google-sheets"
                ],
                "extensions": [
                    ".sheets"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "google-sheets",
                "scopeName": "source.google-sheets",
                "path": "./syntaxes/google-sheets.tmLanguage.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#1e1e1e",
        "theme": "dark"
    },
    "icon": "assets/logo.png",
    "scripts": {
        "start": "npm run build",
        "build": "ts-node ./src/main.ts"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@typescript-eslint/parser": "^5.28.0",
        "colorts": "^0.1.63",
        "eslint": "^8.18.0",
        "fixjson": "^1.1.2",
        "lodash": "^4.17.21",
        "ts-node": "^10.8.1",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4"
    }
}
